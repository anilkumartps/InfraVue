{
  "name": "infravue",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build-client": "npm run build --prefix InfraVue_webpage",
    "start-server": "npm --prefix server start",
    "start": "npm run build-client && npm run start-server",
    "dev": "concurrently \"npm:dev-client\" \"npm:dev-server\"",
    "dev-client": "npm run dev --prefix InfraVue_webpage",
    "dev-server": "npm --prefix server start"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "concurrently": "^8.2.0"
  }
}
